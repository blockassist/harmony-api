{
  "name": "blockassist-harmony",
  "version": "0.0.0",
  "description": "BlockAssist's wrapper for Harmony chain",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "devDependencies": {
    "@types/jest": "~27.0.2",
    "@types/node": "~16.11.6",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "~5.7.0",
    "@typescript-eslint/parser": "~5.7.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "~16.1.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "~25.3.0",
    "jest": "~27.4.4",
    "nodemon": "^2.0.15",
    "prettier": "~2.5.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~27.1.1",
    "ts-node": "^10.4.0",
    "tsutils": "~3.21.0",
    "typescript": "~4.5.3"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "start:dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "tsc && npm run lint"
  },
  "author": "Brian Vogelgesang <kida001@users.noreply.github.com>",
  "dependencies": {
    "@sentry/node": "^6.17.2",
    "axios": "^0.25.0",
    "bech32-converting": "^1.0.8",
    "dotenv": "14.3.2",
    "eth-log-parser": "^0.1.0",
    "express": "^4.17.2",
    "firebase-admin": "^10.0.2",
    "redis": "^4.0.3",
    "tslib": "~2.3.1",
    "web3": "^1.7.0"
  }
}
